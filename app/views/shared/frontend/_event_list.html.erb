<table class="table">
  <thead>
    <tr>
      <th scope="col">活動名稱</th>
      <th scope="col">活動日期</th>
      <th scope="col">活動狀態</th>
    </tr>
  </thead>
  <tbody>
    <% events.each do |event| %>
    <tr>
      <td><%= event.event_name %></td>
      <td><%= "#{event.event_start} ~ #{event.event_end}" %></td>
      <td><%= event.event_status %></td>
    </tr>
    <% end %>
  </tbody>
</table>